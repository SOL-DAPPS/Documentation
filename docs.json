{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Launchpad Trade",
  "description": "The fastest Solana Trading API. Create, trade, and manage tokens with speed, security, and efficiency.",
  
  "colors": {
    "primary": "#7C86FF",
    "light": "#FAFAFA",
    "dark": "#0E0B16"
  },

  "favicon": "/favicon.svg",
  
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://launchpad.trade",
    "height": 40
  },

  "navbar": {
    "links": [
      {
        "label": "Blog",
        "href": "https://launchpad.trade/blog"
      },
      {
        "label": "Status",
        "href": "https://status.launchpad.trade"
      }
    ],
    "primary": {
